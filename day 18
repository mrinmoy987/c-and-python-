#include <stdio.h>
int main() {
    int num, sum = 0;
    printf("Enter a number: ");
    scanf("%d", &num);
    while (num != 0) {
        sum += num % 10;
        num /= 10;
    }
    printf("Sum of digits: %d", sum);
    return 0;
}
num = input("Enter a number: ")
print("Sum of digits:", sum(int(digit) for digit in num))
