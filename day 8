#include <stdio.h>
int main() {
    int num, rev = 0;
    printf("Enter a number: ");
    scanf("%d", &num);
    while (num != 0) {
        rev = rev * 10 + num % 10;
        num /= 10;
    }
    printf("Reversed: %d", rev);
    return 0;
}

num = int(input("Enter a number: "))
rev = 0
while num != 0:
    rev = rev * 10 + num % 10
    num //= 10
print("Reversed:", rev)
