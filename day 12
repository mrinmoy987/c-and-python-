#include <stdio.h>
int main() {
    int arr[5], min;
    printf("Enter 5 numbers: ");
    for (int i = 0; i < 5; i++)
        scanf("%d", &arr[i]);
    min = arr[0];
    for (int i = 1; i < 5; i++) {
        if (arr[i] < min)
            min = arr[i];
    }
    printf("Minimum: %d", min);
    return 0;
}
arr = list(map(int, input("Enter numbers: ").split()))
print("Minimum:", min(arr))
